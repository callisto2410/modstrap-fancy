{
  "name": "@modstrap/fancy",
  "version": "1.0.0",
  "description": "Adaptation for jQuery.FancyBox.",
  "main": "dist/Fancy.js",
  "types": "dist/Fancy.d.ts",
  "style": "dist/Fancy.css",
  "author": "Callisto (https://github.com/callisto2410)",
  "license": "MIT",
  "homepage": "https://github.com/callisto2410/modstrap-fancy",
  "bugs": {
    "url": "https://github.com/callisto2410/modstrap-fancy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callisto2410/modstrap-fancy.git"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:production && npm run build:demo",
    "build:production": "tsc --project tsconfig.json && npx sass --no-source-map src/Fancy.scss dist/Fancy.css",
    "build:demo": "webpack --config webpack.config.js",
    "build:demo:watch": "webpack --watch --config webpack.config.js",
    "build:clean": "npx rimraf .cache dist demo/dist *.tgz",
    "build:pack": "npm run build:clean && npm run build:production && npm pack"
  },
  "devDependencies": {
    "@types/fancybox": "^3.5.2",
    "@types/jquery": "^3.5.4",
    "@types/node": "^14.14.10",
    "autoprefixer": "^10.0.4",
    "css-loader": "^5.0.1",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@modstrap/lexicon": "git+https://github.com/callisto2410/modstrap-lexicon.git#v1.0.0",
    "jquery": "^3.5.1"
  }
}
